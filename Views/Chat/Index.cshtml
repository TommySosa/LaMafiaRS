@{
    ViewData["Title"] = "Salas de chats";
}
<div class="row">
    <div class="col-md-6 offset-2">
        <h2>Salas de chat</h2>
        <ul class="list-group">
            @foreach (var e in LaMafiaRS.Controllers.ChatController.Rooms)
            {
                <li class="list-group-item"><a href="~/Chat/Room?room=@e.Key">@e.Value</a></li>
            }
        </ul>
    </div>
</div>

<a class="text-center text-white m-5 btn btn-dark" href="/Home/Index">Volver al inicio</a>
